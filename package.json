{
  "name": "voting",
  "version": "1.0.0",
  "description": "a voting app",
  "main": "index.js",
  "scripts": {
    "start": "concurrently --kill-others --raw \"npm run start-server\" \"npm run start-client\"",
    "start-server": "cd server && npm start",
    "start-client": "cd client && npm start",
    "test-server": "cd server && npm test",
    "test-client": "cd client && npm test",
    "heroku-postbuild": "cd server && npm install; cd ../server && npm install"
  },
